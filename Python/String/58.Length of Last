# Solution 1:
class Solution(object):
    def lengthOfLastWord(self, s):
        s = s.strip().split()
        if not s:
            return 0

        return len(s[-1])


# Solution 2:
class Solution(object):
    def lengthOfLastWord(self, s):
        s = s.strip()
        length = 0

        for i in range(len(s) - 1, -1, -1):
            if s[i] == " ":
                break
            length += 1
    
        return length
